    <!-- http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/smw#HatGeowissenschaftlichesSpezialgebiet_Objekt -->

    <owl:DatatypeProperty rdf:about="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/smw#HatGeowissenschaftlichesSpezialgebiet_Objekt">
