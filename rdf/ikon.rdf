    <!-- Status -->

    <owl:DatatypeProperty rdf:about="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon#Status">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon#Tätigkeit"/>
        <rdfs:range rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon#StatusLevel"/>
        <rdfs:isDefinedBy rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/smw#"/>
        <rdfs:label xml:lang="en">Status</rdfs:label>
        <rdfs:label xml:lang="de">Status</rdfs:label>
        <rdfs:comment xml:lang="de">Der Bearbeitungsstatus im Redaktionssystem.
	;[[Erlaubt Wert::Neu]](Neu / Entwurf)
	:Ein neuer Artikel wurde angelegt
	;[[Erlaubt Wert::Überarbeiten]] (In Bearbeitung / in Überarbeitung)
	:Der Artikel wird ausgearbeitet / der Artikel wird überarbeitet.
	;[[Erlaubt Wert::Bereit]] (Zur Überprüfung / zur Begutachtung)
	:Der Artikel wurde zur Begutachtung eingereicht. Details zur Bearbeitung werden in der Statusmeldung angegeben.
	;[[Erlaubt Wert::Freigegeben]] (Überprüft)
	:Der Artikel wurde freigegeben. Der Name der freigebenden Person und das Datum werden in der Statusmeldung angegeben.
	</rdfs:comment>
    </owl:DatatypeProperty>
    
    <!-- Statusmeldung -->

    <owl:DatatypeProperty rdf:about="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon#Statusmeldung">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon#Tätigkeit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:isDefinedBy rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/smw#"/>
        <rdfs:label xml:lang="en">Message</rdfs:label>
        <rdfs:label xml:lang="de">Meldung</rdfs:label>
        <rdfs:comment xml:lang="de">
	  Meldungen werden nur in den Bearbeitungsstatus „Überarbeiten“ und „Freigegeben“ angezeigt.
	</rdfs:comment>
    </owl:DatatypeProperty>

