    <owl:Ontology rdf:about="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon">
      <dc:title xml:lang="en">IKON Ontology, semantic MediaWiki fork</dc:title>
      <dc:title xml:lang="de">IKON Ontologie, Fork für Sematic MediWiki</dc:title>
      <dc:description xml:lang="en">A RDF vocabulary for describing knowledge transfer activities at research organisations.</dc:description>
      <dc:description xml:lang="de">RDF Vokabular zur Beschreibung der Wissenstransferaktivitäten in Forschungsorganisationen.</dc:description>
      <rdfs:isDefinedBy>Jonas Oppenlaender, Freie Universität Berlin</rdfs:isDefinedBy>
      <rdfs:isDefinedBy>Alvaro Ortiz-Troncoso, Museum für Naturkunde Berlin</rdfs:isDefinedBy>
      <rdfs:isDefinedBy>Nicolo Knapp, Freie Universität Berlin</rdfs:isDefinedBy>
      <vann:preferredNamespacePrefix xml:lang="en">ikon</vann:preferredNamespacePrefix>
      <owl:versionIRI rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/smw-2.0"/>
      <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2.0</owl:versionInfo>
      <owl:backwardCompatibleWith rdf:resource="http://ikon.code.naturkundemuseum.berlin/ontologies/ikon/1.3.0"/>
    </owl:Ontology>

