#### MediaWiki and API settings ###
# see https://gitlab.com/alvarosaurus/RDF-to-SemanticWiki/wikis/home
# use './configure' to generate this file

[defaults]
# Base URL of the MediaWiki installation (should start with http:// and end with a slash.)
BASE_MW_URL=@@BASE_MW_URL@@
# Username of the MediaWiki API bot user
API_USERNAME=@@API_USERNAME@@
# Password of the MediaWiki API bot user
API_PASS=@@API_PASS@@
# Template path, relative to the SemanticDAO package.
TPL_DIR=@@TPL_DIR@@
